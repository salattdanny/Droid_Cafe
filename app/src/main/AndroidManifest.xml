<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.salatt.droidcafe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mylogo_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_mylogo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Droid_Cafe">
        <activity android:name="com.salatt.droidcafe.DonutActivity" />
        <activity android:name="com.salatt.droidcafe.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>